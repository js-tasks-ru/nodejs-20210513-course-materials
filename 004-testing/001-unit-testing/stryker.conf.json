{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "cleanTempDir": true,
  "files": [
    "./*.js",
    "./src/*.js",
    "./*.yaml"
  ],
  "mutate": [
    "./src/*.js",
    "!./**/*.test.js"
  ],
  "packageManager": "npm",
  "reporters": [
    "html",
    "clear-text",
    "progress"
  ],
  "testRunner": "mocha",
  "coverageAnalysis": "perTest"
}
